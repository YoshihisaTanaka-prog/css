<p style="color: green"><%= notice %></p>

<%= render @title %>

<div>
  <%= link_to "Edit this title", edit_title_path(@title) %> |
  <%= link_to "Back to titles", titles_path %>

  <%= button_to "Destroy this title", @title, method: :delete %>
</div>

<h2>Category List</h2>
<% @title.categories.each do |category| %>
  <%= category.name + " " %>
<% end %>